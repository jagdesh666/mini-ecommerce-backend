[Unit]
Description=gunicorn daemon
After=network.target

[Service]
User=pppjagdesh1234
Group=www-data
WorkingDirectory=/home/pppjagdesh1234/mini-ecommerce-backend
ExecStart=/home/pppjagdesh1234/.virtualenvs/myenv/bin/gunicorn \
          --workers 3 \
          --bind unix:/home/pppjagdesh1234/mini-ecommerce-backend/ecommerce.sock \
          core.wsgi:application

[Install]
WantedBy=multi-user.target